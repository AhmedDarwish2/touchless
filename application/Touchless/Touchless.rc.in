#include "winver.h"

#ifdef RC_INVOKED

1 24 "Touchless.exe.manifest"

#define VER_FILEVERSION        @LEAP_VERSION_MAJOR@,@LEAP_VERSION_MINOR@,@LEAP_VERSION_PATCH@,0
#define VER_FILEVERSION_STR    "@LEAP_VERSION_MAJOR@.@LEAP_VERSION_MINOR@.@LEAP_VERSION_PATCH@\0"
#define VER_PRODUCTVERSION     @LEAP_VERSION_MAJOR@,@LEAP_VERSION_MINOR@,@LEAP_VERSION_PATCH@,0
#define VER_PRODUCTVERSION_STR "@LEAP_VERSION_MAJOR@.@LEAP_VERSION_MINOR@.@LEAP_VERSION_PATCH@\0"

//
// Set up debug information
//
#if DBG
#define VER_DBG VS_FF_DEBUG
#else
#define VER_DBG 0
#endif

IDI_ICON1               ICON    DISCARDABLE     "touchless-icon.ico"

// ------- version info -------------------------------------------------------

VS_VERSION_INFO VERSIONINFO
FILEVERSION VER_FILEVERSION
PRODUCTVERSION VER_PRODUCTVERSION
FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
FILEFLAGS VER_DBG|VS_FF_PRERELEASE
FILEOS VOS_NT
FILETYPE VFT_APP
FILESUBTYPE VFT2_DRV_SYSTEM
BEGIN
 BLOCK "StringFileInfo"
 BEGIN
  BLOCK "040904b0"
        BEGIN
  VALUE "Comments", "Touchless"
  VALUE "CompanyName", "Leap Motion, Inc."
  VALUE "FileDescription", "Touchless"
  VALUE "FileVersion", VER_FILEVERSION_STR
  VALUE "InternalName", "Touchless"
  VALUE "LegalCopyright", "(C) 2012-2013 Leap Motion, Inc."
  VALUE "OriginalFilename", "Touchless.exe"
  VALUE "ProductName", "Touchless"
  VALUE "ProductVersion", VER_PRODUCTVERSION_STR
        END
 END
 BLOCK "VarFileInfo"
 BEGIN
  VALUE "Translation", 0x0409,1200
 END
END
#endif

